<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.csstudio.utility.pvmanager.datasource">
      <datasource
            datasource="org.csstudio.utility.pvmanager.dim.DimDataSourceFromPreferences"
            name="Dim DataSource (dim)"
            prefix="dim">
      </datasource>
   </extension>

   <extension
         point="org.eclipse.help.toc">
      <toc
            file="toc.xml"
            primary="true">
      </toc>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="org.csstudio.platform.ui.css.platform"
            class="org.csstudio.utility.pvmanager.dim.PreferencePage"
            id="org.csstudio.utility.pvmanager.dim.preferencepage"
            name="DIM">
      </page>
   </extension>

</plugin>
